pipeline {
    agent any
    stages {
        stage('Code Quality') {
            steps {
                echo 'Start Build'
            }
        }
        
        stage('Build LMS') {
            steps {
                echo 'Start Testing'
            }
        }
        
        stage('Release LMS') {
            steps {
                echo 'Start Deployment'
            }
        }

        stage('Deploy LMS') {
            steps {
                echo 'Start Deployment'
            }
        }

        stage('CleanUp LMS') {
            steps {
                echo 'Start Deployment'
            }
        }

    }
}